{
  "type": "project",
  "require": {
    "php": "^8.2",
    "ext-pdo": "*",
    "guzzlehttp/guzzle": "^6.5",
    "symfony/console": "^6.4",
    "symfony/dotenv": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/yaml": "^6.4",
    "symfony/twig-bundle": "^6.4",
    "symfony/runtime": "^7.2",
    "symfony/monolog-bundle": "^3.10",
    "symfony/expression-language": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "mockery/mockery": "^1.6",
    "symfony/var-dumper": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  }
}
